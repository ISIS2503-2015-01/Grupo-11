# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.DoctorController.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

#Actualizar doctor 
#EJExito:http://localhost:9000/doctor {"nombre": "Juan","apellido": "Perez","identificacion": "3","fechaNacimiento": "02/14/1995"}
#EJError:http://localhost:9000/doctor {"nombre": "Juan","apellido": "Perez","identificacion": "90","fechaNacimiento": "02/14/1995"}

PUT /doctor						controllers.DoctorController.actualizarDoctor()
