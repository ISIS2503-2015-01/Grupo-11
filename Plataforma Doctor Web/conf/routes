# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.DoctorController.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

#Actualizar doctor 
#EJExito:http://localhost:9000/doctor/2 {"nombre": "Juan","apellido": "Perez","fechaNacimiento": "02/14/1995"}
#EJError:http://localhost:9000/doctor/90 {"nombre": "Juan","apellido": "Perez","fechaNacimiento": "02/14/1995"}

PUT /doctor/:identificacion						controllers.DoctorController.actualizarDoctor(identificacion:String)

POST /doctor									controllers.DoctorController.agregarDoctor()

#Felipe

GET /doctor/:idDoctor/pacientes/:idPaciente		controllers.DoctorController.getPacienteDoctor(idDoctor:String,idPaciente:String)
POST /doctor/:idDoctor/colegas/:idColega/segundaOpinion		controllers.DoctorController.publicarSegundaOpinionEpisodio(idDoctor:String,idColega:String)
GET /paciente/:idPaciente/episodios/:fecha1&:fecha2			controllers.PacienteController.getEpisodiosPeriodo(idPaciente:String,fecha1:String,fecha2:String)
POST /paciente/:idPaciente/episodio/					controllers.PacienteController.agregarEpisodioPaciente(idPaciente:String)
